mponents/index.js",[],"/Users/kalibry/dev/news-explorer-frontend/src/components/NewsCardList/NewsCardList.js",["94"],"import React from \"react\";\nimport \"./NewsCardList.css\";\nimport { NewsCard, Button } from \"../index\";\n\nconst NewsCardList = ({ cards }) => {\n  const [quantity, setQuantity] = React.useState(3);\n\n  const [width, setWidth] = React.useState(window.innerWidth);\n\n  React.useEffect(() => {\n    let cleanupFunction = false;\n    window.addEventListener(\"resize\", () => {\n      setWidth(window.innerWidth);\n    });\n    return () => cleanupFunction = true;\n  }, []);\n\n  let textLength;\n  let titleLength;\n\n  if (width <= 320) {\n    textLength = 90;\n    titleLength = 30;\n  } else if (width <= 768) {\n    textLength = 75;\n    titleLength = 30;\n  } else {\n    textLength = 186;\n    titleLength = 40;\n  }\n\n  function showMoreCards() {\n    setQuantity(6)\n  }\n\n  return (\n    <div className=\"news-cards__container\">\n      <h2 className=\"news-cards__title\">Результаты поиска</h2>\n      <ul className=\"news-cards__list\">\n        {cards.slice(0, quantity).map((item) => (\n          <NewsCard\n            key={item.id}\n            img={item.img}\n            title={\n              item.title.length > titleLength\n                ? `${item.title.substring(0, titleLength)}...`\n                : `${item.title.substring(0, titleLength)}`\n            }\n            text={\n              item.text.length > textLength\n                ? `${item.text.substring(0, textLength)}...`\n                : `${item.text.substring(0, textLength)}`\n            }\n            source={item.source}\n          />\n        ))}\n      </ul>\n      <Button place={\"search-results\"} text={\"Показать еще\"} color={\"white\"} handleBtnClick={showMoreCards}/>\n    </div>\n  );\n};\n\nexport default NewsCardList;\n","/Users/kalibry/dev/news-explorer-frontend/src/data/data.js",["95"],"import cardForestLights from './card-forest-lights.jpg';\nimport cardNatu