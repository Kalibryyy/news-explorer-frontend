   });\n    return () => cleanupFunction = true;\n  }, []);\n\n  let textLength;\n  let titleLength;\n\n  if (width <= 480) {\n    textLength = 90;\n    titleLength = 30;\n  } else if (width <= 768) {\n    textLength = 70;\n    titleLength = 25;\n  } else if (width <= 1024) {\n    textLength = 90;\n    titleLength = 17;\n  }else if (width <= 1280) {\n    textLength = 115;\n    titleLength = 25;\n  } else {\n    textLength = 157;\n    titleLength = 40;\n  }\n\n  function showMoreCards() {\n    setQuantity(6)\n  }\n\n  return (\n    <div className=\"news-cards__container\">\n      <h2 className=\"news-cards__title\">{title}</h2>\n      <ul className=\"news-cards__list\">\n        {cards.slice(0, quantity).map((item) => (\n          <NewsCard\n            key={item.id}\n            img={item.img}\n            title={\n              item.title.length > titleLength\n                ? `${item.title.substring(0, titleLength)}...`\n                : `${item.title.substring(0, titleLength)}`\n            }\n            text={\n              item.text.length > textLength\n                ? `${item.text.substring(0, textLength)}...`\n                : `${item.text.substring(0, textLength)}`\n            }\n            source={item.source}\n            date={item.date}\n            keyword={item.keyword}\n            link={item.link}\n            main={main}\n          />\n        ))}\n      </ul>\n      {doNeedBtn && <Button place={\"search-results\"} text={\"Показать еще\"} color={\"white\"} handleBtnClick={showMoreCards} />}\n    </div>\n  );\n};\n\nexport default NewsCardList;\n","/Users/kalibry/dev/news-explorer-frontend/src/data/data.js",["110"],"import cardForestLights from './card-forest-lights.jpg';\nimport cardNature from './card-nature.jpg';\nimport cardPlant from './card-plant.jpg';\nimport cardPlants from './card-plants.jpg';\nimport cardTaigaPlant from './card-taiga-plant.jpg';\nimport cardTaiga from './card-taiga.jpg';\n\nexport default [{\n    title: 'Национальн